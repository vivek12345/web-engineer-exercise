    "mocha": "^5.2.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "puppeteer": "^1.18.0",
    "style-loader": "^1.0.0",
    "tailwindcss": "^1.1.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.11",
    "webpack-manifest-plugin": "^2.0.4"
